{
  "name": "geobit",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:emu": "cross-env NEXT_PUBLIC_USE_FIREBASE_EMULATORS=true next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "deploy": "next build && firebase deploy",
    "emulators": "firebase emulators:start",
    "check:emulators": "node scripts/check-emulators.js",
    "dev:full": "concurrently \"npm run emulators\" \"npm run dev:emu\""
  },
  "dependencies": {
    "@babel/runtime": "^7.26.10",
    "axios": "^1.6.2",
    "dotenv": "^16.4.5",
    "firebase": "^11.3.0",
    "next": "14.0.3",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5"
  },
  "devDependencies": {
    "@types/react": "19.0.10",
    "autoprefixer": "^10.0.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8",
    "eslint-config-next": "14.0.3",
    "postcss": "^8",
    "tailwindcss": "^3.3.0"
  },
  "optionalDependencies": {
    "@tanstack-query-firebase/react": "^1.0.5",
    "firebase-admin": "^13.2.0",
    "firebase-functions": "^6.3.2"
  }
}